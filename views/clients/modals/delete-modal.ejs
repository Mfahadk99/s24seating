<form id="deleteForm" action="" method="POST">
  <div
    class="modal fade text-left"
    id="deleteModal"
    tabindex="-1"
    role="dialog"
    aria-hidden="true"
  >
    <div class="modal-dialog" role="document">
      <div class="modal-content">
        <div class="modal-header" style="border-bottom: 0">
          <h4 class="modal-title text-primary">Confirm Delete</h4>
          <button
            type="button"
            class="close"
            data-dismiss="modal"
            aria-label="Close"
          >
            <span aria-hidden="true">&times;</span>
          </button>
        </div>
        <div class="modal-body">
          <h5 id="msg">Are you sure you want to delete this client?</h5>
        </div>
        <div class="modal-footer" style="border-top: 1px solid #f1f3f6">
          <button
            type="button"
            class="btn grey btn-outline-secondary"
            data-dismiss="modal"
          >
            Close
          </button>
          <button type="submit" class="btn btn-outline-primary">Delete</button>
        </div>
      </div>
    </div>
  </div>
</form>

<!-- action attribute is change by deleteUser under public/js/pages/accountManager.filter.js -->
