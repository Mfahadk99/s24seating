  <script src="/public/js/core/app-menu.js"></script>
  <script src="/public/js/core/app.js"></script>
  <!-- END: Theme JS-->

  <!-- BEGIN: Page JS-->
  <script src="/public/js/scripts/forms/form-login-register.js"></script>
  <!-- END: Page JS-->
</body>
<!-- END: Body-->

</html>