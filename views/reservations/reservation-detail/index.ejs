<!doctype html>
<html class="loading" lang="en" data-textdirection="ltr">
<!-- BEGIN: Head-->

<head>
    <!-- head partial -->
    <%- include('../../partials/head', {title: restaurant.name}) %>
        <!-- styles partial -->
        <%- include('../../partials/styles.ejs') %>
            <!-- Restaurant Detail Page Specific CSS -->
            <link rel="stylesheet" href="/css/pages/restaurant-detail.css">
            <style>
                /* Hero Section */
                .restaurant-hero {
                    position: relative;
                    height: 400px;
                    background-size: cover;
                    background-position: center;
                    color: white;
                    margin-bottom: 30px;
                    border-radius: 0 0 15px 15px;
                    overflow: hidden;
                }

                .hero-overlay {
                    position: absolute;
                    top: 0;
                    left: 0;
                    width: 100%;
                    height: 100%;
                    background: linear-gradient(to bottom, rgba(0, 0, 0, 0.2) 0%, rgba(0, 0, 0, 0.7) 100%);
                    display: flex;
                    align-items: flex-end;
                    padding: 30px;
                }

                .hero-content {
                    width: 100%;
                }

                .restaurant-name {
                    font-size: 2.5rem;
                    font-weight: 700;
                    margin-bottom: 10px;
                    text-shadow: 2px 2px 4px rgba(0, 0, 0, 0.5);
                }

                .restaurant-meta {
                    display: flex;
                    flex-wrap: wrap;
                    gap: 20px;
                    margin-bottom: 15px;
                }

                .meta-item {
                    display: flex;
                    align-items: center;
                    font-size: 1rem;
                }

                .meta-item i {
                    margin-right: 8px;
                    font-size: 1.2rem;
                }

                /* Rating */
                .rating {
                    display: flex;
                    align-items: center;
                    color: #FFD700;
                    font-size: 1.2rem;
                }

                .rating-text {
                    color: white;
                    margin-left: 10px;
                }

                /* Status Badge */
                .status-badge {
                    padding: 6px 15px;
                    border-radius: 30px;
                    font-weight: 600;
                    font-size: 0.9rem;
                    display: inline-block;
                }

                /* Content Sections */
                .content-section {
                    background-color: white;
                    border-radius: 10px;
                    box-shadow: 0 5px 15px rgba(0, 0, 0, 0.05);
                    padding: 25px;
                    margin-bottom: 30px;
                }

                .section-title {
                    font-size: 1.5rem;
                    font-weight: 600;
                    margin-bottom: 20px;
                    padding-bottom: 15px;
                    border-bottom: 1px solid #eee;
                    color: #333;
                }

                /* Gallery */
                .gallery-container {
                    display: grid;
                    grid-template-columns: repeat(3, 1fr);
                    gap: 15px;
                    margin-bottom: 20px;
                }

                .gallery-item {
                    height: 200px;
                    border-radius: 8px;
                    overflow: hidden;
                    position: relative;
                    cursor: pointer;
                    transition: transform 0.3s;
                }

                .gallery-item:hover {
                    transform: scale(1.02);
                }

                .gallery-item img {
                    width: 100%;
                    height: 100%;
                    object-fit: cover;
                }

                /* Menu Items */
                .menu-category {
                    margin-bottom: 25px;
                }

                .category-title {
                    font-size: 1.3rem;
                    font-weight: 600;
                    margin-bottom: 15px;
                    color: #444;
                }

                .menu-items {
                    display: grid;
                    grid-template-columns: repeat(auto-fill, minmax(300px, 1fr));
                    gap: 20px;
                }

                .menu-item {
                    display: flex;
                    background-color: #f9f9f9;
                    border-radius: 8px;
                    overflow: hidden;
                    transition: transform 0.3s, box-shadow 0.3s;
                }

                .menu-item:hover {
                    transform: translateY(-5px);
                    box-shadow: 0 8px 15px rgba(0, 0, 0, 0.1);
                }

                .menu-image {
                    width: 100px;
                    height: 100px;
                    object-fit: cover;
                }

                .menu-details {
                    padding: 15px;
                    flex: 1;
                }

                .menu-name {
                    font-weight: 600;
                    margin-bottom: 5px;
                }

                .menu-description {
                    font-size: 0.9rem;
                    color: #666;
                    margin-bottom: 10px;
                }

                .menu-price {
                    font-weight: 700;
                    color: #5A8DEE;
                }

                /* Reviews */
                .review {
                    border-bottom: 1px solid #eee;
                    padding-bottom: 20px;
                    margin-bottom: 20px;
                }

                .review:last-child {
                    border-bottom: none;
                    margin-bottom: 0;
                    padding-bottom: 0;
                }

                .review-header {
                    display: flex;
                    align-items: center;
                    margin-bottom: 10px;
                }

                .reviewer-avatar {
                    width: 50px;
                    height: 50px;
                    border-radius: 50%;
                    object-fit: cover;
                    margin-right: 15px;
                }

                .reviewer-info {
                    flex: 1;
                }

                .reviewer-name {
                    font-weight: 600;
                    margin-bottom: 5px;
                }

                .review-date {
                    font-size: 0.8rem;
                    color: #888;
                }

                .review-rating {
                    color: #FFD700;
                    font-size: 1rem;
                    margin-bottom: 10px;
                }

                .review-text {
                    color: #555;
                    line-height: 1.6;
                }

                /* Reservation Form */
                .reservation-form .form-group {
                    margin-bottom: 20px;
                }

                .reservation-form label {
                    font-weight: 600;
                    margin-bottom: 8px;
                    color: #444;
                }

                .reservation-form .btn-primary {
                    padding: 12px 25px;
                    font-weight: 600;
                    margin-top: 10px;
                    border-radius: 30px;
                }

                /* Info List */
                .info-list {
                    list-style: none;
                    padding: 0;
                    margin: 0;
                }

                .info-list li {
                    display: flex;
                    margin-bottom: 15px;
                    align-items: flex-start;
                }

                .info-list i {
                    color: #5A8DEE;
                    margin-right: 15px;
                    font-size: 1.2rem;
                    width: 20px;
                    text-align: center;
                    margin-top: 3px;
                }

                .info-content {
                    flex: 1;
                }

                .info-label {
                    font-weight: 600;
                    margin-bottom: 5px;
                    display: block;
                }

                /* Map */
                .map-container {
                    height: 300px;
                    border-radius: 8px;
                    overflow: hidden;
                    margin-top: 20px;
                }

                /* Responsive */
                @media (max-width: 992px) {
                    .gallery-container {
                        grid-template-columns: repeat(2, 1fr);
                    }
                }

                @media (max-width: 768px) {
                    .restaurant-hero {
                        height: 300px;
                    }

                    .restaurant-name {
                        font-size: 2rem;
                    }

                    .gallery-container {
                        grid-template-columns: 1fr;
                    }

                    .menu-items {
                        grid-template-columns: 1fr;
                    }
                }
            </style>
</head>
<!-- BEGIN: Body-->

<body class="vertical-layout vertical-content-menu 2-columns fixed-navbar" data-open="click"
    data-menu="vertical-content-menu" data-col="2-columns">
    <!-- top-menu partial -->
    <%- include('../../partials/top-menu.ejs') %>

        <!-- BEGIN: Content-->
        <div class="app-content content">
            <div class="content-wrapper">
                <!-- Restaurant Hero Section -->
                <div class="restaurant-hero"
                    style="background-image: url('<%= restaurant.image || '/public/images/placeholder-plot.png' %>');">
                    <div class="hero-overlay">
                        <div class="hero-content">
                            <h1 class="restaurant-name">
                                <%= restaurant.name %>
                            </h1>
                            <div class="restaurant-meta">
                                <div class="meta-item">
                                    <i class="la la-map-marker"></i>
                                    <span>
                                        <%= restaurant.fullAddress || (restaurant.address ?
                                            `${restaurant.address.street}, ${restaurant.address.city},
                                            ${restaurant.address.state}` : 'Address not available' ) %>
                                    </span>
                                </div>
                                <div class="meta-item">
                                    <i class="la la-utensils"></i>
                                    <span>
                                        <%= restaurant.businessType || 'Restaurant' %>
                                    </span>
                                </div>
                                <div class="meta-item">
                                    <i class="la la-phone"></i>
                                    <span>
                                        <%= restaurant.phone || 'Phone not available' %>
                                    </span>
                                </div>
                            </div>

                            <div class="d-flex align-items-center">
                                <div class="rating">
                                    <% for(let i=0; i < 5; i++) { %>
                                        <% if(i < 4) { %>
                                            <i class="la la-star"></i>
                                            <% } else { %>
                                                <i class="la la-star-half-o"></i>
                                                <% } %>
                                                    <% } %>
                                                        <span class="rating-text">4.5 (120 reviews)</span>
                                </div>

                                <div class="ml-auto">
                                    <% if (restaurant.operatingHours && restaurant.operatingHours.isOpen) { %>
                                        <span class="status-badge badge-success">Open Now</span>
                                        <% } else { %>
                                            <span class="status-badge badge-danger">Closed</span>
                                            <% } %>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="container">
                    <div class="row">
                        <!-- Left Column - Main Content -->
                        <div class="col-lg-8">
                            <!-- About Section -->
                            <div class="content-section">
                                <h3 class="section-title">About</h3>
                                <p>
                                    <%= restaurant.description || 'No description available for this restaurant.' %>
                                </p>

                                <!-- Sample description if none exists -->
                                <% if (!restaurant.description) { %>
                                    <p>Welcome to <%= restaurant.name %>, where culinary excellence meets warm
                                            hospitality. Our restaurant offers a unique dining experience with a focus
                                            on fresh, locally-sourced ingredients and innovative recipes that celebrate
                                            the rich traditions of <%= restaurant.businessType || 'culinary' %> cuisine.
                                    </p>
                                    <p>Whether you're joining us for a casual lunch, a romantic dinner, or a special
                                        celebration, our dedicated team is committed to providing exceptional service
                                        and unforgettable meals in a comfortable, inviting atmosphere.</p>
                                    <% } %>
                            </div>

                            <!-- Gallery Section -->
                            <div class="content-section">
                                <h3 class="section-title">Gallery</h3>
                                <div class="gallery-container">
                                    <!-- Sample gallery images -->
                                    <div class="gallery-item">
                                        <img src="<%= restaurant.image || '/public/images/placeholder-plot.png' %>"
                                            alt="Restaurant Interior">
                                    </div>
                                    <div class="gallery-item">
                                        <img src="/public/images/placeholder-plot.png" alt="Restaurant Food">
                                    </div>
                                    <div class="gallery-item">
                                        <img src="/public/images/placeholder-plot.png" alt="Restaurant Ambiance">
                                    </div>
                                    <div class="gallery-item">
                                        <img src="/public/images/placeholder-plot.png" alt="Special Dish">
                                    </div>
                                    <div class="gallery-item">
                                        <img src="/public/images/placeholder-plot.png" alt="Outdoor Seating">
                                    </div>
                                    <div class="gallery-item">
                                        <img src="/public/images/placeholder-plot.png" alt="Chef Special">
                                    </div>
                                </div>
                            </div>

                            <!-- Menu Section -->
                            <div class="content-section">
                                <h3 class="section-title">Menu</h3>

                                <!-- Appetizers -->
                                <div class="menu-category">
                                    <h4 class="category-title">Appetizers</h4>
                                    <div class="menu-items">
                                        <div class="menu-item">
                                            <img src="/public/images/placeholder-plot.png" class="menu-image"
                                                alt="Appetizer">
                                            <div class="menu-details">
                                                <h5 class="menu-name">Bruschetta</h5>
                                                <p class="menu-description">Toasted bread topped with fresh tomatoes,
                                                    basil, and garlic</p>
                                                <div class="menu-price">$8.99</div>
                                            </div>
                                        </div>
                                        <div class="menu-item">
                                            <img src="/public/images/placeholder-plot.png" class="menu-image"
                                                alt="Appetizer">
                                            <div class="menu-details">
                                                <h5 class="menu-name">Calamari</h5>
                                                <p class="menu-description">Crispy fried squid served with marinara
                                                    sauce</p>
                                                <div class="menu-price">$12.99</div>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <!-- Main Courses -->
                                <div class="menu-category">
                                    <h4 class="category-title">Main Courses</h4>
                                    <div class="menu-items">
                                        <div class="menu-item">
                                            <img src="/public/images/placeholder-plot.png" class="menu-image"
                                                alt="Main Course">
                                            <div class="menu-details">
                                                <h5 class="menu-name">Grilled Salmon</h5>
                                                <p class="menu-description">Fresh salmon fillet with lemon herb butter
                                                    and seasonal vegetables</p>
                                                <div class="menu-price">$24.99</div>
                                            </div>
                                        </div>
                                        <div class="menu-item">
                                            <img src="/public/images/placeholder-plot.png" class="menu-image"
                                                alt="Main Course">
                                            <div class="menu-details">
                                                <h5 class="menu-name">Beef Tenderloin</h5>
                                                <p class="menu-description">8oz tenderloin with red wine reduction and
                                                    truffle mashed potatoes</p>
                                                <div class="menu-price">$32.99</div>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <!-- Desserts -->
                                <div class="menu-category">
                                    <h4 class="category-title">Desserts</h4>
                                    <div class="menu-items">
                                        <div class="menu-item">
                                            <img src="/public/images/placeholder-plot.png" class="menu-image"
                                                alt="Dessert">
                                            <div class="menu-details">
                                                <h5 class="menu-name">Tiramisu</h5>
                                                <p class="menu-description">Classic Italian dessert with coffee-soaked
                                                    ladyfingers</p>
                                                <div class="menu-price">$8.99</div>
                                            </div>
                                        </div>
                                        <div class="menu-item">
                                            <img src="/public/images/placeholder-plot.png" class="menu-image"
                                                alt="Dessert">
                                            <div class="menu-details">
                                                <h5 class="menu-name">Chocolate Lava Cake</h5>
                                                <p class="menu-description">Warm chocolate cake with a molten center,
                                                    served with vanilla ice cream</p>
                                                <div class="menu-price">$9.99</div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <!-- Reviews Section -->
                            <div class="content-section">
                                <h3 class="section-title">Reviews</h3>

                                <!-- Sample reviews -->
                                <div class="review">
                                    <div class="review-header">
                                        <img src="/public/images/default-profile.png" class="reviewer-avatar"
                                            alt="Reviewer">
                                        <div class="reviewer-info">
                                            <h5 class="reviewer-name">John Smith</h5>
                                            <div class="review-date">2 days ago</div>
                                        </div>
                                    </div>
                                    <div class="review-rating">
                                        <i class="la la-star"></i>
                                        <i class="la la-star"></i>
                                        <i class="la la-star"></i>
                                        <i class="la la-star"></i>
                                        <i class="la la-star"></i>
                                    </div>
                                    <p class="review-text">Amazing food and excellent service! The atmosphere was
                                        perfect for our anniversary dinner. We especially loved the beef tenderloin and
                                        the chocolate lava cake was to die for. Will definitely be coming back!</p>
                                </div>

                                <div class="review">
                                    <div class="review-header">
                                        <img src="/public/images/default-profile.png" class="reviewer-avatar"
                                            alt="Reviewer">
                                        <div class="reviewer-info">
                                            <h5 class="reviewer-name">Sarah Johnson</h5>
                                            <div class="review-date">1 week ago</div>
                                        </div>
                                    </div>
                                    <div class="review-rating">
                                        <i class="la la-star"></i>
                                        <i class="la la-star"></i>
                                        <i class="la la-star"></i>
                                        <i class="la la-star"></i>
                                        <i class="la la-star-o"></i>
                                    </div>
                                    <p class="review-text">Great place for a business lunch. The service was quick and
                                        the food was delicious. The only reason I'm giving 4 stars instead of 5 is
                                        because the portion sizes were a bit small for the price. Otherwise, highly
                                        recommended!</p>
                                </div>

                                <div class="review">
                                    <div class="review-header">
                                        <img src="/public/images/default-profile.png" class="reviewer-avatar"
                                            alt="Reviewer">
                                        <div class="reviewer-info">
                                            <h5 class="reviewer-name">Michael Chen</h5>
                                            <div class="review-date">2 weeks ago</div>
                                        </div>
                                    </div>
                                    <div class="review-rating">
                                        <i class="la la-star"></i>
                                        <i class="la la-star"></i>
                                        <i class="la la-star"></i>
                                        <i class="la la-star"></i>
                                        <i class="la la-star-half-o"></i>
                                    </div>
                                    <p class="review-text">I've been to <%= restaurant.name %> multiple times and it
                                            never disappoints. The menu has a great variety and the staff is always
                                            friendly and attentive. Their seasonal specials are always worth trying!</p>
                                </div>
                            </div>
                        </div>

                        <!-- Right Column - Sidebar -->
                        <div class="col-lg-4">
                            <!-- Reservation Form -->
                            <div class="content-section">
                                <h3 class="section-title">Make a Reservation</h3>

                                <!-- Initial Reservation Form -->
                                <div id="reservation-initial-form">
                                    <div class="form-group">
                                        <label for="party-size">Party Size</label>
                                        <select class="form-control" id="party-size" required>
                                            <option value="">Select party size</option>
                                            <option value="1">1 person</option>
                                            <option value="2">2 people</option>
                                            <option value="3">3 people</option>
                                            <option value="4">4 people</option>
                                            <option value="5">5 people</option>
                                            <option value="6">6 people</option>
                                            <option value="7">7 people</option>
                                            <option value="8">8 people</option>
                                        </select>
                                    </div>
                                    <div class="form-group">
                                        <label for="reservation-date">Date</label>
                                        <input type="date" class="form-control" id="reservation-date" required>
                                    </div>
                                    <div class="form-group">
                                        <label for="time-slot">Time Slot</label>
                                        <select class="form-control" id="time-slot" required>
                                            <option value="">Select a time slot</option>
                                            <!-- Time slots will be loaded dynamically -->
                                        </select>
                                        <small class="form-text text-muted">Available time slots for this restaurant</small>
                                    </div>
                                    <input type="hidden" id="restaurant-id" value="<%= restaurant._id %>">
                                    <div id="search-slots-container">
                                        <button type="button" id="search-available-slots"
                                            class="btn btn-primary btn-block">Find Available Tables</button>
                                    </div>
                                    <div id="reservation-search-error" class="alert alert-danger mt-3"
                                        style="display: none;"></div>
                                </div>

                                <!-- Available Time Slots -->
                                <div id="available-slots-container" style="display: none;">
                                    <h4 class="mb-3">Available Times</h4>
                                    <p class="mb-3">Select a time for your party of <span
                                            id="selected-party-size"></span> on <span
                                            id="selected-date-display"></span><span id="selected-time-display"></span>
                                    </p>

                                    <div id="time-slots" class="d-flex flex-wrap mb-4">
                                        <!-- Time slots will be dynamically inserted here -->
                                    </div>

                                    <button type="button" id="back-to-search" class="btn btn-outline-secondary">Change
                                        Search</button>

                                    <div class="mt-3">
                                        <small class="text-muted">
                                            <i class="la la-info-circle"></i> Booked <span id="booked-count">0</span>
                                            times today
                                        </small>
                                    </div>
                                </div>

                                <!-- Reservation Confirmation Modal -->
                                <div class="modal fade" id="reservationModal" tabindex="-1" role="dialog"
                                    aria-labelledby="reservationModalLabel" aria-hidden="true">
                                    <div class="modal-dialog" role="document">
                                        <div class="modal-content">
                                            <div class="modal-header">
                                                <h5 class="modal-title" id="reservationModalLabel">Complete Your
                                                    Reservation</h5>
                                                <button type="button" class="close" data-dismiss="modal"
                                                    aria-label="Close">
                                                    <span aria-hidden="true">&times;</span>
                                                </button>
                                            </div>
                                            <div class="modal-body">
                                                <div class="reservation-summary mb-4">
                                                    <h6>Reservation Details</h6>
                                                    <div class="d-flex align-items-center mb-2">
                                                        <i class="la la-users mr-2" style="width: 20px;"></i>
                                                        <span id="modal-party-size"></span>
                                                    </div>
                                                    <div class="d-flex align-items-center mb-2">
                                                        <i class="la la-calendar mr-2" style="width: 20px;"></i>
                                                        <span id="modal-date"></span>
                                                    </div>
                                                    <div class="d-flex align-items-center mb-3">
                                                        <i class="la la-clock-o mr-2" style="width: 20px;"></i>
                                                        <span id="modal-time"></span>
                                                    </div>
                                                </div>

                                                <form id="reservation-confirmation-form">
                                                    <input type="hidden" id="selected-table-id">
                                                    <input type="hidden" id="selected-start-time">
                                                    <input type="hidden" id="selected-end-time">
                                                    <input type="hidden" id="selected-slot-id">

                                                    <div class="form-group">
                                                        <label for="contact-name">Name</label>
                                                        <input type="text" class="form-control" id="contact-name"
                                                            placeholder="Your name" required>
                                                    </div>
                                                    <div class="form-group">
                                                        <label for="contact-email">Email</label>
                                                        <input type="email" class="form-control" id="contact-email"
                                                            placeholder="Your email" required>
                                                    </div>
                                                    <div class="form-group">
                                                        <label for="contact-phone">Phone</label>
                                                        <input type="tel" class="form-control" id="contact-phone"
                                                            placeholder="Your phone number" required>
                                                    </div>
                                                    <div class="form-group">
                                                        <label for="special-requests">Special Requests
                                                            (Optional)</label>
                                                        <textarea class="form-control" id="special-requests" rows="3"
                                                            placeholder="Any special requests or dietary requirements?"></textarea>
                                                    </div>

                                                    <div id="reservation-error" class="alert alert-danger"
                                                        style="display: none;"></div>
                                                    <div id="reservation-success" class="alert alert-success"
                                                        style="display: none;">Reservation confirmed! We look forward to
                                                        serving you.</div>
                                                </form>
                                            </div>
                                            <div class="modal-footer">
                                                <button type="button" class="btn btn-secondary"
                                                    data-dismiss="modal">Cancel</button>
                                                <button type="button" id="complete-reservation"
                                                    class="btn btn-primary">Complete Reservation</button>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <!-- Restaurant Info -->
                            <div class="content-section">
                                <h3 class="section-title">Restaurant Info</h3>
                                <ul class="info-list">
                                    <li>
                                        <i class="la la-clock-o"></i>
                                        <div class="info-content">
                                            <span class="info-label">Hours</span>
                                            <div>
                                                <% if (restaurant.operatingHours) { %>
                                                    <%= restaurant.operatingHours.openTime || '9:00 AM' %> - <%=
                                                            restaurant.operatingHours.closeTime || '10:00 PM' %>
                                                            <% } else { %>
                                                                9:00 AM - 10:00 PM
                                                                <% } %>
                                            </div>
                                        </div>
                                    </li>
                                    <li>
                                        <i class="la la-phone"></i>
                                        <div class="info-content">
                                            <span class="info-label">Phone</span>
                                            <div>
                                                <%= restaurant.phone || 'Not available' %>
                                            </div>
                                        </div>
                                    </li>
                                    <li>
                                        <i class="la la-envelope"></i>
                                        <div class="info-content">
                                            <span class="info-label">Email</span>
                                            <div>
                                                <%= restaurant.email || 'Not available' %>
                                            </div>
                                        </div>
                                    </li>
                                    <li>
                                        <i class="la la-map-marker"></i>
                                        <div class="info-content">
                                            <span class="info-label">Address</span>
                                            <div>
                                                <%= restaurant.fullAddress || (restaurant.address ?
                                                    `${restaurant.address.street}, ${restaurant.address.city},
                                                    ${restaurant.address.state} ${restaurant.address.zipCode}`
                                                    : 'Address not available' ) %>
                                            </div>
                                        </div>
                                    </li>
                                    <li>
                                        <i class="la la-utensils"></i>
                                        <div class="info-content">
                                            <span class="info-label">Cuisine</span>
                                            <div>
                                                <%= restaurant.businessType || 'Various cuisines' %>
                                            </div>
                                        </div>
                                    </li>
                                </ul>

                                <!-- Map placeholder -->
                                <div class="map-container">
                                    <iframe width="100%" height="100%" frameborder="0" style="border:0"
                                        src="https://www.google.com/maps/embed/v1/place?key=AIzaSyA0s1a7phLN0iaD6-UE7m4qP-z21pH0eSc&q=<%= encodeURIComponent(restaurant.fullAddress || (restaurant.address ? `${restaurant.address.street}, ${restaurant.address.city}` : restaurant.name)) %>"
                                        allowfullscreen>
                                    </iframe>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
